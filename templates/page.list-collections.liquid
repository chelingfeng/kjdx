<div class="central">
  <div class="content {% unless settings.coll_content_bg_show %}transparent{% endunless %}">
    {% if page.content != blank %}
    <h1 class="feature-header" data-cc-animate>{{ page.title }}</h1>
    <div class="rte" data-cc-animate data-cc-animate-delay="0.2s">
      {{ page.content }}
    </div>
    {% else %}
    <h1 class="no-margin" data-cc-animate>{{ page.title }}</h1>
    {% endif %}
  </div>
</div>

{% section 'page-list-collections-template' %}

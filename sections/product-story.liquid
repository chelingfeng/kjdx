<div data-section-type="nested-sections">
  {% for block in section.blocks %}
    {% if block.type == 'rich-text' %}
      <div class="shopify-section section-rich-text" {{ block.shopify_attributes }}>{% assign brightness = block.settings.color_bg | color_brightness %}
{% assign alpha = block.settings.color_bg | color_extract: 'alpha' %}

<div id="section-id-{{ block.id }}" class="section-id-{{ block.id }} cf {% if block.settings.alternate_bg_color %}use-alt-bg {% elsif block.settings.color_bg != "rgba(0,0,0,0)" %}{% if brightness < 190 and alpha > 0 %}light-text-section{% else %}dark-text-section{% endif %}{% endif %}" data-cc-animate>
  {% style %}
    {% unless block.settings.alternate_bg_color %}
      .section-id-{{ block.id }} {
        background-color: {{ block.settings.color_bg }};
      }
    {% endunless %}

    #section-id-{{ block.id }} .section-rich-text__image .rimage-outer-wrapper {
      width: {{ block.settings.image_width }}px;
    }

    @media (min-width: 768px) {
      #section-id-{{ block.id }} .feature-header {
        {%- assign title_size_medium = block.settings.title_size | times: 0.7 -%}
        font-size: {{ title_size_medium }}px;
      }
    }

    @media (min-width: 992px) {
      #section-id-{{ block.id }} .feature-header {
        font-size: {{ block.settings.title_size }}px;
      }
    }
  {% endstyle %}

  <div class="section-spacing wide-container">
    <div class="content transparent align-{{ block.settings.text_alignment }} {% if block.settings.full_width == false %}reading-width{% endif %}">

      {% if block.settings.image != blank %}
        <div class="section-rich-text__image" data-cc-animate="cc-fade-in-up">
          {%- render 'responsive-image', image: block.settings.image -%}
        </div>
      {% endif %}

      {% if block.settings.subheading != blank %}
        <div class="feature-subheader" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.1s">{{ block.settings.subheading | escape }}</div>
      {% endif %}

      {% if block.settings.title != blank %}
        <h2 class="feature-header align-{{ block.settings.text_alignment }}
          {% if block.settings.title_size >= 86 %}feature-header--large{% endif %}
          {% if block.settings.text == blank and block.settings.button_label == blank %}feature-header__no-line feature-header__no-gap{% endif %}" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.2s">
          {{ block.settings.title | newline_to_br }}
        </h2>
      {% endif %}

      {% if block.settings.text != blank %}
        <div class="rte inherit-colors {% if block.settings.enlarge_text %}large-text{% endif %} align-{{ block.settings.text_alignment }}" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.3s">
          {{ block.settings.text }}
        </div>
      {% endif %}

      {% if block.settings.button_label != blank %}
        <div class="button-row inherit-colors" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.3s">
          <a data-cc-animate-click class="
            {% if block.settings.button_style != 'auto' %}button-custom {% endif %}
            {% if block.settings.button_style == 'link' %}link-display {% if block.settings.enlarge_text %}type-2{% endif %}{% else %}button {% if block.settings.button_style == 'secondary' %}alt{% endif %}{% endif %}" href="{{ block.settings.button_link }}">
            {{ block.settings.button_label | escape }}
          </a>
        </div>
      {% endif %}
    </div>
  </div>
</div>


</div>

    {% elsif block.type == 'image-with-text' %}
      <div class="shopify-section section-image-with-text" {{ block.shopify_attributes }}>{% assign brightness = block.settings.color_bg | color_brightness %}
{% assign alpha = block.settings.color_bg | color_extract: 'alpha' %}
<style type="text/css">
  {% unless block.settings.alternate_bg_color %}
  .section-id-{{ block.id }} {
    background: {{ block.settings.color_bg }};
  }
  {% endunless %}

  @media(min-width: 768px) {
    .section-id-{{ block.id }} .image-with-text__image__inner {
      {% if block.settings.media_size == "cover" %}
        transform: scale({{ block.settings.image_width | times: 0.01 }});
      {% elsif block.settings.media_size == "contain" %}
        height: auto !important;
        width: {{ block.settings.image_width }}% !important;
      {% endif %}
    }

    .section-id-{{ block.id }} .image-with-text__text {
      flex-basis: {{ block.settings.text_width }}%;
    }

    .section-id-{{ block.id }} .image-with-text__image__inner .rimage-outer-wrapper {
      background-size: {{ block.settings.media_size }};
    }
  }
</style>

{% capture image_html %}
  <div class="image-with-text__image lazyload--placeholder">
    <div class="image-with-text__image__inner">
      {%- if block.settings.image == blank and block.settings.video == blank -%}
        <div class="placeholder-image">{{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}</div>
      {%- elsif block.settings.video == blank -%}
        <div class="rimage-outer-wrapper lazyload fade-in"
            data-cc-animate="cc-zoom-out"
            data-bgset="{% render 'bgset', image: block.settings.image %}"
            data-sizes="auto"
            data-parent-fit="cover"
            style="max-width: {{ block.settings.image.width }}px; max-height: {{ block.settings.image.height }}px">
          <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%">
            <noscript>
              <img src="{{ block.settings.image | img_url: '1024x1024' }}" alt="{{ block.settings.image.alt | escape }}" class="rimage__image">
            </noscript>
          </div>
        </div>
      {%- else -%}
        {% render 'inline-video', video: block.settings.video, image: block.settings.image %}
      {% endif %}
    </div>
  </div>
{% endcapture %}

<div class="image-with-text cf
  section-id-{{ block.id }}
  {% if block.settings.alternate_bg_color %}use-alt-bg {% elsif block.settings.color_bg != "rgba(0,0,0,0)" %}{% if brightness < 190 and alpha > 0 %}light-text-section{% else %}dark-text-section{% endif %}{% endif %}
  {% if block.settings.image_width < 100 and block.settings.media_size == "contain" %}section-spacing-sm{% endif %}
  image-with-text--{{ block.settings.media_size }}
" data-nested-section-type="image-with-text" data-cc-animate>

  {% if block.settings.image_position == 'left' %}{{ image_html }}{% endif %}

  <div class="image-with-text__text align-{{ block.settings.text_alignment }}">
    <div class="image-with-text__text__wrap
        {% if brightness < 190 and alpha > 0 %}inherit-colors{% endif %}
        {% if block.settings.image_width < 100 and block.settings.media_size == "contain" %}padded-block-y{% else %}padded-block{% endif %}
      ">
      {% if block.settings.subheading != blank %}
        <div class="feature-subheader" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.1s">{{ block.settings.subheading | escape }}</div>
      {% endif %}

      {% if block.settings.title != blank %}
        <h2 class="feature-header h2 align-{{ block.settings.text_alignment }}" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.2s">
          {{ block.settings.title | newline_to_br }}
        </h2>
      {% endif %}

      <div class="rte align-{{ block.settings.text_alignment }}" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.3s">{{ block.settings.text }}</div>
      {% if block.settings.button_label != blank %}

      <div class="button-row" data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.4s">
        {% if block.settings.button_link != blank %}
          <a data-cc-animate-click class="
            {% if block.settings.button_style != 'auto' %}button-custom {% endif %}
            {% if block.settings.button_style == 'link' %}link-display {% else %}button {% if block.settings.button_style == 'secondary' %}alt{% endif %}{% endif %}"
             href="{{ block.settings.button_link }}">
        {% else %}
          <span class="
            {% if block.settings.button_style != 'auto' %}button-custom {% endif %}
            {% if block.settings.button_style == 'link' %}link-display {% else %}button {% if block.settings.button_style == 'secondary' %}alt{% endif %}{% endif %}">
        {% endif %}

          {{ block.settings.button_label | escape }}

        {% if block.settings.button_link == blank %}
          </span>
        {% else %}
          </a>
        {% endif %}
      </div>

      {% endif %}
    </div>
  </div>

  {% if block.settings.image_position == 'right' %}{{ image_html }}{% endif %}

</div>


</div>

    {% elsif block.type == 'background-video' %}
      <div class="shopify-section section-background-video" {{ block.shopify_attributes }}><div id="section-id-{{ block.id }}" class="{% if block.settings.full_width == false %}section-spacing wide-container{% else %}header-overlap-section needs-alt-logo{% endif %}" data-nested-section-type="background-video">
  {% style %}
    {%- if block.settings.height == 'fixed' -%}
    #section-id-{{ block.id }} .video-container {
      height: {{ block.settings.height_mobile }}px;
    }
    {%- endif -%}

    @media (min-width: 768px) {
      {%- if block.settings.height == 'fixed' -%}
      #section-id-{{ block.id }} .video-container {
        height: {{ block.settings.height_desktop }}px;
      }
      {%- endif -%}


    #section-id-{{ block.id }} .line-1 {
        {%- assign title_size_medium = block.settings.title_size | times: 0.7 -%}
        font-size: {{ title_size_medium }}px;
      }
    }

    @media (min-width: 992px) {
      #section-id-{{ block.id }} .line-1 {
        font-size: {{ block.settings.title_size }}px;
      }
    }
  {% endstyle %}

  {% if block.settings.video != blank %}
    <div class="height--{{ block.settings.height }} video-container video-container--background image-overlay  {% if block.settings.overlay_style_tint %}image-overlay--bg-full{% endif %}"
         data-video-url="{{ block.settings.video | escape }}"
         data-video-autoplay="true"
         data-video-loop="true">

      {% if block.settings.overlay_style_tint %}
        <div class="image-overlay__veil" data-cc-animate data-cc-animate-duration="1.5s" data-cc-animate-offset="50%"></div>
      {% endif %}

      <div class="video-container__video"></div>

      {%- assign show_overlay_text = false -%}
      {%- if block.settings.title != blank or block.settings.subheading != blank or block.settings.text != blank or block.settings.button_label != blank -%}
        {%- assign show_overlay_text = true -%}
      {%- endif -%}

      {% if block.settings.placeholder_image != blank %}
        <div class="video-container__fallback rimage-outer-wrapper lazyload fade-in"
            data-bgset="{% render 'bgset', image: block.settings.placeholder_image %}"
            data-sizes="auto"
            data-parent-fit="cover">
          <noscript>
            <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.placeholder_image.aspect_ratio | times: 100 }}%">
              <img src="{{ block.settings.placeholder_image | img_url: '1024x1024' }}" alt="{{ block.settings.placeholder_image.alt | escape }}" class="rimage__image">
            </div>
          </noscript>
        </div>
      {% endif %}

      {%- if show_overlay_text -%}
        <div class="overlay-type overlay {{ block.settings.overlay_position }} small-padding-xs"
             data-cc-animate-offset="50%"
             data-cc-animate-delay=".5s"
             data-cc-animate="cc-fade-in-up">
          <div class="inner">
            <div>
              <div class="overlay__content">
                {%- if block.settings.subheading != blank -%}
                  <div class="feature-subheader">
                      {{ block.settings.subheading | escape }}
                  </div>
                {%- endif -%}
                {%- if block.settings.title != blank -%}
                  <h2 class="line-1  {% if block.settings.title_size >= 86 %}line-1--large{% endif %}">
                      {{ block.settings.title | newline_to_br }}
                  </h2>
                {%- endif -%}
                {%- if block.settings.text != blank -%}
                  <div class="rte line-2 {% if block.settings.enlarge_text %}large-text{% endif %}">
                    {{ block.settings.text }}
                  </div>
                {%- endif -%}
                {%- if block.settings.button_label != blank -%}
                  <div class="line-3">
                    <a data-cc-animate-click class="button hover-grow" href="{{ block.settings.button_link }}">
                      {{ block.settings.button_label | escape }}
                    </a>
                  </div>
                {%- endif -%}
              </div>
            </div>
          </div>
        </div>
      {%- endif -%}
    </div>
  {% endif %}
</div>

</div>

    {% elsif block.type == 'image-with-text-overlay' %}
      <div class="shopify-section header-overlap-section section-image-with-text-overlay needs-alt-logo" {{ block.shopify_attributes }}><div id="section-id-{{ block.id }}" class="section-id-{{ block.id }}" data-nested-section-type="image-with-text-overlay" data-cc-animate>
  {% style %}
    {%- if block.settings.height == 'fixed' -%}
    #section-id-{{ block.id }} .rimage-outer-wrapper,
    #section-id-{{ block.id }} .placeholder-image {
      height: {{ block.settings.height_mobile }}px;
    }
    {%- endif -%}

    #section-id-{{ block.id }} .rimage-outer-wrapper {
      background-position: {{ block.settings.mobile_image_position }}
    }

    @media (min-width: 768px) {
      {%- if block.settings.height == 'fixed' -%}
      #section-id-{{ block.id }} .rimage-outer-wrapper ,
      #section-id-{{ block.id }} .placeholder-image {
        height: {{ block.settings.height_desktop }}px;
      }
      {%- endif -%}

      #section-id-{{ block.id }} .line-1 {
        {%- assign title_size_medium = block.settings.title_size | times: 0.7 -%}
        font-size: {{ title_size_medium }}px;
      }

      #section-id-{{ block.id }} .rimage-outer-wrapper {
        background-position: {{ block.settings.desktop_image_position }}
      }
    }

    @media (min-width: 992px) {
      #section-id-{{ block.id }} .line-1 {
        font-size: {{ block.settings.title_size }}px;
      }
    }
  {% endstyle %}

  {%- unless block.settings.full_width -%}<div class="wide-container half-gutter section-spacing">{%- endunless -%}

    {%- assign show_overlay_text = false -%}
    {%- if block.settings.title != blank or block.settings.subheading != blank or block.settings.text != blank or block.settings.button_label != blank -%}
      {%- assign show_overlay_text = true -%}
    {%- endif -%}

    <div class="height--{{ block.settings.height }} image-overlay {% if block.settings.overlay_style_tint %}image-overlay--bg-full{% endif %}
{% if block.settings.mobile_overlay_under %}overlay-text-static-xs{% endif %}">
      {%- unless block.settings.image_link == blank or block.settings.text contains '</a>' -%}<a href="{{ block.settings.image_link }}">{%- endunless -%}

        {% if block.settings.image == blank %}
          <div class="placeholder-image placeholder--banner {% if show_overlay_text %}dark{% endif %}">
            {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' }}
          </div>
        {% else %}
          <div class="rimage-outer-wrapper rimage-background lazyload fade-in"
              data-cc-animate="cc-zoom-out"
              data-bgset="{% render 'bgset', image: block.settings.image %}"
              data-sizes="auto"
              data-parent-fit="cover">
            {%- if block.settings.height == 'natural' -%}
              <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%"></div>
            {%- endif -%}
            <noscript>
              <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%">
                <img src="{{ block.settings.image | img_url: '1024x1024' }}" alt="{{ block.settings.image.alt | escape }}" class="rimage__image">
              </div>
            </noscript>
          </div>
        {% endif %}

        {%- if show_overlay_text -%}
          <div class="overlay-type overlay {{ block.settings.overlay_position }}">
            <div class="inner">
              <div>
                <div class="overlay__content" {% if block.settings.overlay_position == 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-zoom-out"{% endif %}>
                  {%- if block.settings.subheading != blank -%}
                    <div class="feature-subheader" {% if block.settings.overlay_position != 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.3s"{% endif %}>
                      {{ block.settings.subheading | escape }}
                    </div>
                  {% endif %}

                  {%- if block.settings.title != blank -%}
                    <h2 class="line-1  {% if block.settings.title_size >= 86 %}line-1--large{% endif %}"
                        {% if block.settings.overlay_position != 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.2s"{% endif %}>
                      {{ block.settings.title | newline_to_br }}
                    </h2>
                  {%- endif -%}

                  {%- if block.settings.text != blank -%}
                    <div class="rte line-2 {% if block.settings.enlarge_text %}large-text{% endif %}" {% if block.settings.overlay_position != 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.4s"{% endif %}>
                      {{ block.settings.text }}
                    </div>
                  {%- endif -%}

                  {%- if block.settings.button_label != blank -%}
                    <div class="line-3" {% if block.settings.overlay_position != 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-up" data-cc-animate-delay="0.5s"{% endif %}>
                      <span class="button hover-grow">{{ block.settings.button_label | escape }}</span>
                    </div>
                  {%- endif -%}
                </div>
              </div>
            </div>
          </div>
        {% endif %}

      {%- unless block.settings.image_link == blank or block.settings.text contains '</a>' -%}</a>{%- endunless -%}
    </div>

  {%- unless block.settings.full_width -%}</div>{%- endunless -%}
</div>

</div>

    {% elsif block.type == 'video' %}
      <div class="shopify-section section-video" {{ block.shopify_attributes }}><div id="section-id-{{ block.id }}" class="{% if block.settings.full_width == false %}section-spacing wide-container{% else %}header-overlap-section needs-alt-logo{% endif %}" data-nested-section-type="video">
  {% style %}
    #section-id-{{ block.id }} .rimage-outer-wrapper {
      background-position: {{ block.settings.mobile_image_position }}
    }

    @media (min-width: 768px) {
      #section-id-{{ block.id }} .line-1 {
        {%- assign title_size_medium = block.settings.title_size | times: 0.7 -%}
        font-size: {{ title_size_medium }}px;
      }

      #section-id-{{ block.id }} .rimage-outer-wrapper {
        background-position: {{ block.settings.desktop_image_position }}
      }
    }

    @media (min-width: 992px) {
      #section-id-{{ block.id }} .line-1 {
        font-size: {{ block.settings.title_size }}px;
      }
    }
  {% endstyle %}

  {% if block.settings.video != blank %}
    <div class="video-container video-container--image video-container--modal-mobile image-overlay  {% if block.settings.overlay_style_tint %}image-overlay--bg-full{% endif %}"
         data-cc-animate
         data-video-url="{{ block.settings.video }}"
         data-video-id="{{ block.settings.video.id }}"
         data-video-type="{{ block.settings.video.type }}"
         data-video-autoplay="false"
         data-video-loop="false">
      <a class="video-container__stop" href="#">{% render 'svg-x' %}</a>
      <div class="video-container__video"></div>

      {%- assign show_overlay_text = true -%}

      <div class="video-container__cover">
        {%- if block.settings.image == blank -%}
          <div class="placeholder-image placeholder--banner {% if show_overlay_text %}dark{% endif %}">
            {{- 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' -}}
          </div>
        {%- else -%}
          <div class="rimage-outer-wrapper lazyload fade-in"
              data-bgset="{% render 'bgset', image: block.settings.image %}"
              data-sizes="auto"
              data-parent-fit="cover">
            <noscript>
              <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%">
                <img src="{{ block.settings.image | img_url: '1024x1024' }}" alt="{{ block.settings.image.alt | escape }}" class="rimage__image">
              </div>
            </noscript>
          </div>
        {%- endif -%}
      </div>

      {%- if show_overlay_text -%}
        <div class="overlay-type overlay {{ block.settings.overlay_position }}">
          <div class="inner">
            <div>
              <div class="overlay__content" {% if block.settings.overlay_position == 'position--hcenter position--vcenter' %}data-cc-animate="cc-fade-in-zoom-out"{% endif %}>
                {%- if block.settings.subheading != blank -%}
                  <div class="feature-subheader">
                    {{ block.settings.subheading | escape }}
                  </div>
                {%- endif -%}
                {%- if block.settings.video_title != blank -%}
                  <h2 class="line-1 {% if block.settings.title_size >= 86 %}line-1--large{% endif %}">
                    {{ block.settings.video_title | newline_to_br }}
                  </h2>
                {%- endif -%}
                {%- if block.settings.text != blank -%}
                  <div class="rte line-2 {% if block.settings.enlarge_text %}large-text{% endif %}">
                    {{ block.settings.text }}
                  </div>
                {%- endif -%}

                <div class="line-3">
                  <a href="#" class="video-container__play button hover-grow"
                     aria-label="{{ 'general.accessibility_labels.video_play' | t | escape }}">
                    {% render 'svg-play-without-circle' %}
                    {{ 'general.accessibility_labels.video_play' | t | escape }}
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      {%- endif -%}
    </div>
  {% endif %}
</div>


</div>

    {% elsif block.type == 'custom-html' %}
      <div class="shopify-section section--custom-html" {{ block.shopify_attributes }}><div class="section-spacing align-center {% unless block.settings.skip_container %}container{% endunless %}" data-cc-animate>
  {{ block.settings.custom_html }}
</div>

</div>

    {% elsif block.type == 'map' %}
      <div class="shopify-section index-section" {{ block.shopify_attributes }}>{% assign display_map = false %}
{%- if block.settings.api_key != blank and block.settings.map_address -%}
  {% assign display_map = true %}
{%- endif -%}

{% assign brightness = block.settings.color_bg | color_brightness %}
{% assign alpha = block.settings.color_bg | color_extract: 'alpha' %}

<style type="text/css">
  .section-id-{{ block.id }} .map-section__wrapper {
    background: {{ block.settings.color_bg }};
  }

  {% unless block.settings.image == blank %}
    @media screen and (max-width: 749px) {
      .map-section__image--{{ block.id }} {
        padding-top: {{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;
      }
    }
  {% endunless %}
</style>

<div class="section-id-{{ block.id }} section section-spacing map-section{% if display_map %} map-section--display-map{% endif %}" data-nested-section-type="map" data-api-key="{{ block.settings.api_key }}" data-map-style="{{ block.settings.map_style }}">
  <div class="wide-container">
    <div class="map-section__wrapper {% if block.settings.color_bg != "rgba(0,0,0,0)" %}{% if brightness < 190 and alpha > 0 %}light-text-section{% else %}dark-text-section{% endif %}{% endif %}">
      <div class="map-section__content map-section__map">
        {% if block.settings.image %}
          <div class="map-section__image map-section__image--{{ block.id }} supports-js lazyload"
              data-bgset="{% render 'bgset', image: block.settings.image %}"
              data-sizes="auto"
              data-parent-fit="cover">
          </div>
          <noscript>
            <div class="map-section__image  map-section__image--{{ block.id }}" style="background-image: url({{ block.settings.image | img_url: '400x400'}});"></div>
          </noscript>
        {% else %}
          <div class="map-section__placeholder placeholder-image">
            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
          </div>
        {% endif %}
        {% if display_map %}
          <div class="map-section__map-container"
              id="Map-{{ block.id }}"
              data-address-setting="{{ block.settings.map_address | escape }}">
          </div>
        {% endif %}
      </div>

      <div class="map-section__content map-section__text padded-block">

  {
    "name": "Product story",
    "class": "section-product-story",
    "max_blocks": 15,
    "settings": [
      {
        "type": "paragraph",
        "content": "Note: The content above will appear on all products. Per product content is not yet possible on Shopify in theme settings."
      }
    ],
    "blocks": [
      {
        
  "name": "Rich text",
  "type": "rich-text",
  "settings": [
    {
      "type": "text",
      "id": "subheading",
      "label": "Subheading"
    },
    {
      "type": "textarea",
      "id": "title",
      "label": "Heading",
      "default": "Rich text"
    },
    {
      "type": "range",
      "id": "title_size",
      "min": 20,
      "max": 150,
      "step": 2,
      "unit": "px",
      "label": "Heading text size",
      "default": 60
    },
    {
      "type": "richtext",
      "id": "text",
      "label": "Text",
      "default": "<p>Use this text to share information about your store with your customers.</p><p>Describe product, share announcements, or welcome customers to your store.</p>"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    },
    {
      "type": "range",
      "id": "image_width",
      "min": 60,
      "max": 700,
      "step": 10,
      "label": "Image width",
      "unit": "px",
      "default": 120
    },
    {
      "type": "text",
